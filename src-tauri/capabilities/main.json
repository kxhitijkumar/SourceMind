{
  "$schema": "https://schema.tauri.app/config/2/capabilities/desktop/v1",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
     "core:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "http:default"
  ]
}